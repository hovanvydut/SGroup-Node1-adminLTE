{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "indent": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "no-tabs": 0,
        "dot-notation": 0,
        "default-case": 0,
        "no-unused-vars": 0,
        "space-before-function-paren": 0,
        "func-names": 0,
        "no-useless-escape": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "operator-linebreak": 0
    }
}
